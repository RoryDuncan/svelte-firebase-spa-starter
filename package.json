  "devDependencies": {
    "firebase": "^5.8.4",
    "rollup": "^1.2.3",
    "page": "^1.11.4",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-includepaths": "^0.2.3",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-svelte": "^5.0.3",
    "svelte": "^2.16.1"
