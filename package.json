{
  "name": "svelte-firebase-spa-starter",
  "version": "1.0.0",
  "description": "A Svelte.js and Firebase starter project for building an SPA",
  "main": "README.md",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rollup -c",
